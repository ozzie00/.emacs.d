;;; init-multi-term.el --- multiple term -*- lexical-binding: t -*-
;;; Commentary:
;;; Code:


(require 'multi-term)
(setq multi-term-program "/bin/bash")
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(gud-gdb-command-name "gdb --annotate=1")
 '(large-file-warning-threshold nil)
 '(package-selected-packages
   '(swift-mode dash-docs abgaben pomidor rustic cargo rust-mode auctex racket-mode auto-overlays ## org-edna))
 '(session-use-package t nil (session))
 '(tabbar-separator '(0.5))
 '(term-default-bg-color "#000000")
 '(term-default-fg-color "#dddd00")
 '(warning-suppress-log-types '((emacs) (emacs)))
 '(warning-suppress-types '((emacs))))
       ;; foreground color (yellow)


(provide 'init-multi-term)
;;; init-multi-term.el ends here
